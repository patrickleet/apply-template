#!/usr/bin/env node

let loaded = false;

if (loaded) return process.kill();

const program = require('commander');
const debug = require('debug')('meta-template');

program
  // .command('checkout', 'checkout a common branch across all repositories')
  // .command('clone', 'clone meta and child repositories')
  // .command('status', 'template status of meta and child repositories')
  .parse(process.argv);

loaded = true;